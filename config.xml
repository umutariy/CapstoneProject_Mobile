<?xml version='1.0' encoding='utf-8'?>
<widget id="com.brianmartey.eduapp" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>eduapp</name>
    <description>
        An educational app with a difference.
    </description>
    <author email="brian.martey@ashesi.edu.gh" href="#">
        Brian Martey
    </author>
    <icon height="72" src="eduapp.png" width="72" />
    <icon height="48" src="eduapp.png" width="48" />
    <icon height="36" src="eduapp.png" width="36" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
